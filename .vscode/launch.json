  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File with Arguments",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": "${command:pickArgs}"
    },
    {
      "name": "Python Debugger: get-profile 0",
      "type": "debugpy",
      "request": "launch",
      "program": "remapper/cli.py",
      "console": "integratedTerminal",
      "args": "get-profile 0"
    }
  ]
}